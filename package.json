{
  "name": "@naturacosmeticos/natds-rn",
  "version": "0.0.0-development",
  "license": "MIT",
  "main": "build/lib/index.js",
  "author": {
    "name": "Douglas Freitas",
    "email": "douglasfreitas@natura.net",
    "url": "https://github.com/douglasofreitas"
  },
  "description": "Natura Design System for React Native",
  "homepage": "https://github.com/natura-cosmeticos/natds-rn",
  "files": [
    "build/lib"
  ],
  "scripts": {
    "start": "concurrently --names 'auto-import-stories,react-native' 'rnstl' 'react-native start'",
    "start:clear": "concurrently --names 'auto-import-stories,react-native' 'rnstl' 'yarn start --reset-cache'",
    "start:clear:cache": "rm -rf /tmp/metro-* && watchman watch-del-all && yarn start:clear",
    "test": "jest --collectCoverage",
    "test:watch": "jest --watch",
    "test:clean": "jest --update-snapshot",
    "ios:5": "react-native run-ios --simulator=\"iPhone 5s\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix .",
    "android": "concurrently --names 'auto-import-stories,react-native' 'rnstl' 'react-native run-android'",
    "ios": "concurrently --names 'auto-import-stories,react-native' 'rnstl' 'react-native run-ios'",
    "storybook": "start-storybook -p 7007",
    "storybook:web": "node ./node_modules/@storybook/react/bin/index.js start-storybook -p 9001 -c storybook-web -s storybook-web/static",
    "cm": "git-cz",
    "release": "semantic-release",
    "build": "tsc && babel build/lib --out-dir build/lib --ignore node_modules,dist --source-maps --copy-files --no-babelrc --presets react-native",
    "build:sample:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build:sample:ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/NaturaReactComponents/main.jsbundle --assets-dest ios",
    "build:storybook:web": "build-storybook -c storybook-web -o build/storybook -s storybook-web/static",
    "tsc": "tsc --noEmit",
    "publish:storybook:web": "node ./bin/netlify/deploy.js",
    "delete:storybook:web": "node ./bin/netlify/delete.js",
    "postinstall": "npm dedupe && rm package-lock.json",
    "prestorybook": "rnstl"
  },
  "dependencies": {
    "@naturacosmeticos/natds-icons": "^0.4.0",
    "@naturacosmeticos/natds-styles": "^0.4.2",
    "prop-types": "15.7.2",
    "react-native-gesture-handler": "1.6.0",
    "react-native-svg": "11.0.1",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/runtime": "7.6.2",
    "@naturacosmeticos/eslint-config-natura": "2.0.1",
    "@react-native-community/eslint-config": "0.0.5",
    "@storybook/addon-actions": "5.3.18",
    "@storybook/addon-docs": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "5.3.18",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/addons": "5.3.18",
    "@storybook/api": "^5.3.18",
    "@storybook/cli": "^5.3.18",
    "@storybook/components": "^5.3.18",
    "@storybook/core-events": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@storybook/react-native": "5.3.18",
    "@storybook/react-native-server": "5.3.18",
    "@storybook/theming": "^5.3.18",
    "@types/babel__core": "7.1.7",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/eslint": "6.8.0",
    "@types/fs-extra": "8.1.0",
    "@types/jest": "^25.2.1",
    "@types/prop-types": "15.7.2",
    "@types/react": "16.9.0",
    "@types/react-dom": "16.8.3",
    "@types/react-native": "^0.62.2",
    "@types/react-test-renderer": "^16.9.2",
    "@types/semantic-release": "15.13.1",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "babel-cli": "6.26.0",
    "babel-jest": "25.3.0",
    "babel-loader": "8.0.6",
    "babel-plugin-react-native-web": "^0.12.2",
    "babel-preset-react-native": "1.9.2",
    "concurrently": "^5.2.0",
    "cz-conventional-changelog": "3.1.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-mocha": "6.3.0",
    "eslint-plugin-react": "7.18.3",
    "fs-extra": "8.1.0",
    "husky": "1.3.1",
    "jest": "25.3.0",
    "jest-styled-components": "7.0.2",
    "metro-react-native-babel-preset": "0.58.0",
    "netlify": "^4.0.5",
    "react": "16.11.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "16.8.3",
    "react-is": "^16.13.1",
    "react-native": "0.62.2",
    "react-native-storybook-loader": "^1.8.1",
    "react-native-svg-transformer": "0.14.3",
    "react-native-svg-web": "^1.0.1",
    "react-native-web": "^0.12.2",
    "react-test-renderer": "16.11.0",
    "regenerator-runtime": "^0.13.5",
    "semantic-release": "17.0.4",
    "shelljs": "^0.8.3",
    "storybook-addon-designs": "^5.2.0",
    "ts-jest": "^25.4.0",
    "ts-loader": "^7.0.0",
    "typescript": "^3.8.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.59.10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "react-native-storybook-loader": {
      "searchDir": [
        "./src/components"
      ],
      "pattern": "**/*.native.tsx",
      "outputFile": "./storybook/storyLoader.ts"
    }
  }
}
